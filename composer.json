{
  "name": "jascha030/xerox",
  "description": "Framework, heavily inspired by laravel and roots/bedrock, aiming to help relieve your WordPression.",
  "license": "MIT",
  "authors": [
    {
      "name": "Jascha030",
      "email": "contact@jaschavanaalst.nl"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4|^8.0",
    "vlucas/phpdotenv": "^5.3",
    "symfony/console": "^5.3",
    "symfony/finder": "^5.3",
    "symfony/process": "^5.3",
    "doctrine/dbal": "^3.1",
    "twig/twig": "^3.3",
    "php-di/php-di": "^6.3",
    "ext-curl": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "friendsofphp/php-cs-fixer": "^3.1",
    "phpunit/phpunit": "^9.5",
    "composer/composer": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Jascha030\\Xerox\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jascha030\\Xerox\\Tests\\": "tests"
    }
  },
  "bin": [
    "bin/xerographer"
  ]
}
